#child

config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path '10180000.wmac'
	option txpower '20'
	option country '00'
	option disabled '0'
	option channel 'auto'

config wifi-iface
	option ssid 'PBLV_master'
	option encryption 'psk2'
	option password 'password'
	option device 'radio0'
	option mode 'sta'
	option network 'wwan'



